{
  "name": "bavix/router",
  "description": "Component for building site routes. It just works!",
  "keywords": ["routing", "router", "URL", "URI"],
  "homepage": "https://github.com/bavix/router",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Babichev Maxim",
      "email": "info@babichev.net"
    }
  ],
  "require": {
    "ext-json": "*",
    "psr/cache": "^1.0"
  },
  "require-dev": {
    "tedivm/stash": "^0.14 || ^0.15 || ^0.16",
    "bavix/config": "^1.0",
    "symfony/yaml": ">=3.2",
    "bavix/tests": "^1.0",
    "bavix/xml": "^1.0"
  },
  "suggest": {
    "ext-memcached": "*",
    "ext-redis": "*",
    "ext-apcu": "*",
    "ext-xml": "*"
  },
  "autoload": {
    "psr-4": {
      "Bavix\\Router\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
